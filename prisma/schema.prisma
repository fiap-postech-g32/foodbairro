// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Usuario {
  id String @id @default(cuid())
  createdAt DateTime @default(now())
  nome String
  email String
  cpf String
}

model Pedido {
  id String @id @default(cuid())
  createdAt DateTime @default(now())
  numero String
  status Int
}